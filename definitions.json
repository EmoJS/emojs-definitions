{
  // Saved in Ascii compatible definitions, to preserve my sanity
  // When adding characters, please make sure to add a comment with the current name of the stated emoji

  // EmoJS specific escape characters, to allow literal emojis to be expressed in code
  "escapeCharacters" : {
    "singleEscape" : "0x1F42C", // Dolphin
    "openEscape"   : "0x1F449", // White Right Pointing Backhand Index
    "closeEscape"  : "0x1F448" // White Left Pointing Backhand Index
  },
  "singleCharacters": {
    "World"   : "Hello", // Testing pair
    "0x1F300" : "A", // Cyclone
    "0x1F415" : "if", // Dog
    "0x1F408" : "else" // Dog
  }

