{
  // Saved in Ascii compatible hex definitions, to preserve my sanity
  // When adding characters, please make sure to add a comment with the current name of the stated emoji
  "version": "0.2.5",

  // EmoJS specific escape characters
  "escapeCharacters" : {
    // Character for escaping single characters
    "singleEscape" : "0x1F42C", // Dolphin

    // Character for escaping multiple characters TODO: setup implementation structure
    // "openEscape"   : "0x1F449", // White Right Pointing Backhand Index
    // "closeEscape"  : "0x1F448", // White Left Pointing Backhand Index

    // Keycap character used in number key emoji
    "keycap"       : "0x20E3" // Combining Enclosing Keycap
  },
  "singleCharacters": {
    "0x1F311" : "true", // new moon symbol
    "0x1F315" : "false", // full moon symbol
    "0x1F31D" : "NaN", // full moon with face
    "0x1F31A" : "undefined", // new moon with face
    "0x1F319" : "null", // crescent moon
    "0x26A0"  : "alert", // warning sign
    "0x2615"  : "break", // hot beverage
    "0x1F501" : "Infinity", // clockwise rightwards and leftwards open circle arr
    "0x1F389" : "function", // party popper
    "0x1F415" : "if", // Dog
    "0x1F408" : "else", // Cat
    "0x1F4BB" : "console", // Personal Computer
    "0x1F333" : "log", // Deciduous Tree
    "0x1F313" : "[", // First Quarter Moon Symbol
    "0x1F317" : "]", // Last Quarter Moon Symbol
    "0x1F314" : "(", // waxing gibbous moon symbol
    "0x1F316" : ")", // waning gibbous moon symbol
    "0x1F31C" : "{", // last quarter moon with face
    "0x1F31B" : "}", // first quarter moon with face
    "0x26AB"  : ".", // Medium Black Circle
    "0x1F607" : ";", // Smiling Face With Halo
    "0x261D"  : "'", // white up pointing backhand index
    "0x270C"  : "\"", // victory hand
    "0x274C"  : "!=", // cross mark
    "0x2757"  : "!", // heavy exclamation mark symbol
    "0x2716"  : "*", // heavy multiplication x
    "0x2795"  : "+", // heavy plus sign
    "0x2796"  : "-", // heavy minus sign
    "0x2797"  : "/", // heavy division sign
    "0x1F4AF" : "%", // hundred points symbol
    "0x1F51F" : "10", // Keycap 10
    "0x1F300" : "A" // Cyclone
  }

  // TODO: add support for multi-character definitions
  // "multiCharacters": {
  //   "World"          : "Hello" // Testing pair
  //   "0x0031, 0x20E3" : "1" // Keycap 1 
  // }
}